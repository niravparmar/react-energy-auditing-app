    "kue": "KUE_APP=1 node .build/server/index.js",
    "test": "NODE_ENV=api_test jest ",
    "server-test": "NODE_ENV=test mocha -b -R spec .build/server/__tests__/index.js",
    "test-debug": "NODE_ENV=test iron-node node_modules/mocha/bin/_mocha --debug -R spec .build/server/__tests__/index.js",
    "karma": "karma start",
    "migrate": "knex migrate:latest",
    "webpack": "babel-node --presets=es2015 -- server/src/init/webpack-init.js",
    "jest": "NODE_ENV=client_test jest",
